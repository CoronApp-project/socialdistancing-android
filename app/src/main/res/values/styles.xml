<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/homeColorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppTheme.Splash" parent="@style/Theme.AppCompat.NoActionBar">
        <item name="android:windowFullscreen">true</item>
        <item name="android:background">@color/white</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>
    <style name="AppTheme.Home">
        <item name="android:windowFullscreen">true</item>
        <item name="android:background">@color/white</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="colorPrimary">@color/homeColorPrimary</item>
    </style>
    <style name="fabButtonStyle" parent="Widget.Design.FloatingActionButton">
        <item name="maxImageSize">56.5dp</item>
        <item name="background">@drawable/ic_ar_view</item>

    </style>

</resources>
